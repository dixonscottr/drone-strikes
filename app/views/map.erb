<div class='container map-container'>
  <div id="map"></div>
      <script>
        var map;
        function initMap() {
          map = new google.maps.Map(document.getElementById('map'), {
            center: {lat: 19.445, lng: 59.578},
            zoom: 4
          });
        var strikes = mapStrikes(<%= lat_lon(@strikes)%>);
        }
      </script>
      <script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['google_maps_api']%>&callback=initMap" async defer>
      </script>
  </div>
</div>